<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c='urn:import:com.google.gwt.user.cellview.client'>
    <ui:style>
        .products {
            position: relative;
            margin: 1em 10em;
            text-align: center;
        }

        .list > div > div {
            display: inline-block;
        }
    </ui:style>
    <g:RenderablePanel styleName="{style.products}">
        <c:CellList ui:field="products" styleName="{style.list}"></c:CellList>
    </g:RenderablePanel>
</ui:UiBinder>